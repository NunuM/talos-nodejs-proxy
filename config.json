{
  "appId": "d0e06949-fd64-437a-9bfe-ce3cf2d7b63d",
  "numberOfWorkers": 0,
  "proxy": {
    "globalMiddlewares": [
      {
        "type": "AccessLoggingMiddleware",
        "args": {}
      }
    ],
    "servers": [
      {
        "port": 7080,
        "protocol": "http",
        "options": {}
      },
      {
        "port": 7081,
        "protocol": "http2",
        "options": {
          "key": "ssl/key3.pem",
          "cert": "ssl/cert3.pem",
          "allowHTTP1": true
        }
      }
    ],
    "gateways": []
  },
  "administration": {
    "port": 7777,
    "bindOnlyLocalhost": true,
    "authenticator": {
      "type": "none"
    }
  },
  "repository": {
    "type": "file",
    "filePath": "gateways.json"
  },
  "logging": {
    "accessLog": {
      "level": "info",
      "format": ":remote-addr - \":method :url HTTP/:http-version\" :status :content-length \":referrer\" \":user-agent\" :response-time :req[host] \":req[x-correlation-id]\"",
      "logDirectory": "./"
    },
    "proxy": {
      "level": "debug"
    },
    "admin": {
      "level": "info"
    },
    "worker": {
      "level": "info"
    }
  }
}
