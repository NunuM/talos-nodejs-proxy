{
  "source": {
    "includePattern": ".+\\.js(doc|x)?$",
    "include": [
      "."
    ],
    "exclude": [
      "node_modules",
      "ui",
      "test"
    ]
  },
  "plugins": [
    "plugins/markdown"
  ],
  "recurseDepth": 10,
  "opts": {
    "destination": "./docs/",
    "recurse": true,
    "template": "node_modules/docdash",
    "readme": "./README.md"
  },
  "docdash": {
    "openGraph": {
      "title": "Talos NodeJS Reverse Proxy",
      "type": "website",
      "image": "https://image.flaticon.com/icons/svg/1122/1122581.svg",
      "site_name": "Talos NodeJS Reverse Proxy",
      "url": "https://talos.sh"
    },
    "meta": {
      "title": "Talos NodeJS Reverse Proxy",
      "description": "This proxy forwards HTTP requests based on the host header to the user-defined upstreams. One host can have multiple upstreams.",
      "keyword": "reverse proxy, proxy, nodejs, web admin, hot reload, load balancing"
    }
  }
}
